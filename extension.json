	"config": {
		"LayersEnable": {
			"description": "Master switch for Layers extension functionality",
			"value": true
		},
		"LayersDebug": {
			"description": "Enable verbose debug logging to the 'Layers' log channel. Set to true in LocalSettings.php for development.",
			"value": false
		},
		"LayersMaxBytes": {
			"description": "Maximum JSON size per layer set in bytes",
			"value": 2097152
		},
		"LayersMaxLayerCount": {
			"description": "Maximum number of layers per layer set",
			"value": 100
		},
		"LayersMaxComplexity": {
			"description": "Maximum complexity score for a layer set. Each layer type has a cost (text: 2, image: 3, shapes: 1, etc). Total must not exceed this value.",
			"value": 100
		},
		"LayersMaxImageBytes": {
			"description": "Maximum size in bytes for imported image layers (base64 encoded). Default 1MB. Recommended: 512KB-2MB depending on storage capacity.",
			"value": 1048576
		},
		"LayersMaxNamedSets": {
			"description": "Maximum number of named layer sets per image",
			"value": 15
		},
		"LayersMaxRevisionsPerSet": {
			"description": "Maximum revisions to keep per named layer set (older ones are pruned)",
			"value": 50
		},
		"LayersDefaultSetName": {
			"description": "Default name for layer sets when none is specified",
			"value": "default"
		},
		"LayersUseBinaryOverlays": {
			"description": "Enable legacy binary overlay files (.l01, .l02, etc)",
			"value": false
		},
		"LayersDefaultFonts": {
			"description": "Available fonts in the layer editor. Web fonts are loaded from Google Fonts.",
			"value": [
				"Arial",
				"Verdana",
				"Roboto",
				"Open Sans",
				"Lato",
				"Montserrat",
				"Noto Sans",
				"Source Sans Pro",
				"PT Sans",
				"Ubuntu",
				"Times New Roman",
				"Georgia",
				"Merriweather",
				"Playfair Display",
				"Courier New",
				"Source Code Pro"
			]
		},
		"LayersMaxImageSize": {
			"description": "Maximum image size for layer editing in pixels",
			"value": 4096
		},
		"LayersThumbnailCache": {
			"description": "Enable caching of composite thumbnails",
			"value": true
		},
		"LayersImageMagickTimeout": {
			"description": "Timeout in seconds for ImageMagick operations",
			"value": 30
		},
		"LayersMaxImageDimensions": {
			"description": "Maximum width/height for layer processing",
			"value": 8192
		},
		"LayersSlidesEnable": {
			"description": "Enable Slide Mode for creating canvas-based layers without a parent image",
			"value": true
		},
		"LayersSlideDefaultWidth": {
			"description": "Default width for slides when not specified",
			"value": 800
		},
		"LayersSlideDefaultHeight": {
			"description": "Default height for slides when not specified",
			"value": 600
		},
		"LayersSlideMaxWidth": {
			"description": "Maximum allowed slide width",
			"value": 4096
		},
		"LayersSlideMaxHeight": {
			"description": "Maximum allowed slide height",
			"value": 4096
		},
		"LayersSlideDefaultBackground": {
			"description": "Default background color for slides",
			"value": "#ffffff"
		}
	},
