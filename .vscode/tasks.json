{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "npm test (Layers)",
			"type": "shell",
			"command": "npm",
			"args": [
				"test"
			],
			"isBackground": false,
			"group": "build",
			"problemMatcher": [
				"$eslint-stylish"
			],
			"close": true
		},
		{
			"label": "npm test:php (Layers)",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"test:php"
			],
			"isBackground": false,
			"group": "build",
			"close": true
		},
		{
			"label": "npm fix:php (Layers)",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"fix:php"
			],
			"isBackground": false,
			"group": "build",
			"close": true
		},
		{
			"label": "Run Jest Tests",
			"type": "shell",
			"command": "npm run test:js",
			"group": "test"
		},
		{
			"label": "Run HistoryManager Tests",
			"type": "shell",
			"command": "npm run test:js -- --testPathPattern=HistoryManager",
			"group": "test"
		},
		{
			"label": "Run HistoryManager Tests v2",
			"type": "shell",
			"command": "npm run test:js -- HistoryManager",
			"group": "test"
		},
		{
			"label": "Run All Jest Tests",
			"type": "shell",
			"command": "npm run test:js",
			"group": "test"
		}
	]
}